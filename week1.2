#include <stdio.h>
#include <stdlib.h>

int compare(const void *a, const void *b) {
    return (*(int *)a - *(int *)b);
}

void sortProductIDs(int ids[], int n) {
    qsort(ids, n, sizeof(int), compare);
}

int main() {
    int n, i;

    printf("Enter the number of product IDs: ");
    scanf("%d", &n);

    int ids[n];
    printf("Enter Product IDs separated by spaces:\n");
    for (i = 0; i < n; i++) {
        scanf("%d", &ids[i]);
    }

    sortProductIDs(ids, n);

    printf("After Sorting Product IDs:\n");
    for (i = 0; i < n; i++) {
        if (i > 0) printf(",");  // Print comma before each element except the first
        printf("%d", ids[i]);
    }
    printf("\n");

    return 0;
}
