#include <stdio.h>
#include <stdlib.h>

int compare(const void *a, const void *b) {
    return (*(int *)a - *(int *)b);
}

void sortOrders(int priorities[], int n) {
    qsort(priorities, n, sizeof(int), compare);
}

int main() {
    int n, i;

    printf("Enter the number of orders: ");
    scanf("%d", &n);

    int priorities[n];
    printf("Enter orders Priority Number separated by spaces:\n");
    for (i = 0; i < n; i++) {
        scanf("%d", &priorities[i]);
    }

    sortOrders(priorities, n);

    printf("After Sorting Orders Priority:\n");
    for (i = 0; i < n; i++) {
        if (i > 0) printf(",");  // Print comma before each element except the first
        printf("%d", priorities[i]);
    }
    printf("\n");

    return 0;
}
