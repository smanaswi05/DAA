#include <stdio.h>
#include <stdlib.h>

int compareAsc(const void *a, const void *b) {
    return (*(int *)a - *(int *)b);
}

int compareDesc(const void *a, const void *b) {
    return (*(int *)b - *(int *)a);
}

void sortDeliveryTimes(int times[], int n, int ascending) {
    if (ascending) {
        qsort(times, n, sizeof(int), compareAsc);
    } else {
        qsort(times, n, sizeof(int), compareDesc);
    }
}

int main() {
    int n, i, ascending;

    printf("Enter the number of destinations: ");
    scanf("%d", &n);

    int times[n];
    printf("Enter time to reach destination:\n");
    for (i = 0; i < n; i++) {
        scanf("%d", &times[i]);
    }

    printf("Enter 1 for ascending order or 0 for descending order: ");
    scanf("%d", &ascending);

    sortDeliveryTimes(times, n, ascending);

    printf("OUTPUT:\n");
    for (i = 0; i < n; i++) {
        printf("%d\n", times[i]);
    }

    return 0;
}
